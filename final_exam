###Q6
f = np.zeros((10,10))
f[1:-1,1:-1] = 1
print(f)
###Q7
g = np.ones((3,3))
print(g)
g = np.zeros((8,8),dtype=int)
g[1::2,::2] = 1
g[::2,1::2] = 1
print(g)
